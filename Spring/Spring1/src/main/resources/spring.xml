<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="alien1" class="com.telusko.Alien">
<!--        Constructor Injection-->
        <constructor-arg type="int" value="21"></constructor-arg>
        <constructor-arg type="com.telusko.Laptop" ref="lap1"></constructor-arg>
    </bean>
    <bean id="lap1" class="com.telusko.Laptop">
        <!-- we also need this bean as well to make lap1 object work-->
    </bean>
    <bean id="lap2" class="com.telusko.Laptop">
        <!-- we also need this bean as well to make lap1 object work-->
    </bean>
</beans>
